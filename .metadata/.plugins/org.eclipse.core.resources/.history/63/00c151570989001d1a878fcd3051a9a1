

public class Hello {
	public static void main(String[] args) {

         int a=3;
         int b=88;
         
         System.out.println("a= "+a);
         System.out.println("b= "+b);
         
        float c= (float)a;
        float d=(float)b +2,5;
        
        System.out.println("------\nc= "+c);
        System.out.println("d= "+d);
        
        String C = "Heloo";
        String chuoi ="chupi";
        System.out.println("Ep String ="+C);
        System.out.println("Ep String ="+chuoi);
        
        int h= (int)d;
        System.out.println("So h= "+h);
	}
}
