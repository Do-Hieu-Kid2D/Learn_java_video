import java.util.Scanner;

public class Hello {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
         int a=3;
         int b=88;
         
         System.out.println("Nhap a= ");
         a= sc.nextInt();
         System.out.println("Nhap b= ");
         b= sc.nextInt();
         int sum = a+b;
         System.out.println("Sum= "+ a+b);
         System.out.println("Sum= "+ sum);

        
	}
}
